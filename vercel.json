{
    "version": 2,
    "builds": [
        {
            "src": "front/src/App.jsx",
            "use": "@vercel/static-build"
        }
    ],
    "functions": {
        "api": {
            "handler": "api/index.js"
        }
    },
    "routes": [
        { "handle": "filesystem" },
        { "src": "/*", "dest": "api/index.js" }
      ]
}